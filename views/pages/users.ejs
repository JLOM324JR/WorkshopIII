<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>EMAIL</th>
            <th>PASSWORD</th>
            <th><a name="" id="" class="btn btn-success" href="/add_user" role="button">Add New</a>
            </th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user) {%>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
        </tr>
    <%}); %>
    </tbody>
</table>


<% include ../partials/footer %>