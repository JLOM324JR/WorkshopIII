<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>EMAIL</th>
            <th>PASSWORD</th>
            <th>tr</th>
            <th>tuu</th>
            <th>po</th>
        </tr>
    </thead>
    <tbody id="myTable">
    <% users.forEach(function(user) {%>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= perchases_item.perchases_id %></td>
            <td><%= perchases_item.product_id %></td>
            <td><%= perchases_item.price %></td>
            <td><%= perchases_item.quantity %></td>
            <td><%= perchases_item.state %></td>
         
        </tr>
    <%}); %>
    </tbody>
</table>

<script>
$(document).ready(function(){
  $("#myInput").on("keyup", function() {
    var value = $(this).val().toLowerCase();
    $("#myTable tr").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  });
});
</script>


<% include ../partials/footer %>