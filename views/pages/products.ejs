<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>TITLE</th>
            <th>PRICE</th>
            <th><a name="" id="" class="btn btn-success" href="/add_product" role="button">Add New</a>
            </th>
        </tr>
    </thead>
    <tbody>
    <% products.forEach(function(product) {%>
        <tr>
            <td scope="row"><%= product.id %></td>
            <td><%= product.title %></td>
            <td><%= product.price %></td>
            <td><a name="" id="" class="btn btn-primary" href="/products/<%= product.id %>" role="button">Edit</a></td>
        </tr>
    <%}); %>
    </tbody>
</table>


<% include ../partials/footer %>